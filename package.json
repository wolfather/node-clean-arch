{
  "name": "ts-buildings",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.dev.json",
    "dev": "nodemon",
    "pretest": "jest --clearCache",
    "dev:test": "jest --watchAll --detectOpenHandles --colors --verbose",
    "test": "jest --passWithNoTests --silent",
    "start": "node dist/server.js",
    "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/server.js --project tsconfig.dev.json",
    "start:pm2": "pm2 start ./dist/server.js --env production",
    "prepare": "husky install"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "backend",
    "api"
  ],
  "author": "Israel <so.israelweb@gmail.com>",
  "maintainers": [
    "Israel"
  ],
  "license": "ISC",
  "repository": {
    "type": "github",
    "url": "https://github.com/wolfather/ts-buildings"
  },
  "dependencies": {
    "@prisma/client": "4.8.0",
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dayjs": "1.11.7",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.0",
    "uuid": "9.0.0",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.17",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "husky": "^8.0.0",
    "jest": "^29.0.3",
    "jest-globals": "^0.1.7",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
